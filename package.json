{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",

  "scripts": {
    "predev": "kill-port 3000",
    "dev": "cross-env NODE_ENV=development nodemon server/index.cjs",
    "dev:3005": "cross-env PORT=3005 nodemon server/index.cjs",
    "dev:client": "vite",
    "dev:both": "concurrently -n server,client -c auto \"npm:dev\" \"npm:dev:client\"",
    "server": "node server/index.cjs",
    "build": "vite build --config ./vite.config.mts",
    "preview": "vite preview",
    "build:client": "vite build --config ./vite.config.mts",
    "preview:client": "vite preview --config ./vite.config.mts",
    "start": "cross-env NODE_ENV=production node server/index.cjs",
    "health": "node -e \"require('http').get('http://localhost:'+(process.env.PORT||3000)+'/api/health',r=>{console.log('status',r.statusCode)})\"",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "rotate": "node server/tools/rotate.cjs",
    "backup": "node server/tools/backup.cjs",
    "prune": "node server/tools/prune.cjs",
    "restore:check": "node server/tools/restore-check.cjs"
  },

